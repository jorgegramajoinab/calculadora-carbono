@model IEnumerable<model.Models.MathExpression>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear nueva expreción matemática", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Código especie");
        </th>
        <th>
            @Html.DisplayName("Nombre");
        </th>
        <th>
            @Html.DisplayName("Expreción mantemática");
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Species.code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Expression)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit",  new { id=item.Id }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.Id }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
